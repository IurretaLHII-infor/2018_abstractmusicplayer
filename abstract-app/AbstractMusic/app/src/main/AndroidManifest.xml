<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bortu.abstractmusic">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AtMain"></activity>
        <activity android:name=".AtAlbum"></activity>
        <activity android:name=".AtArtist"></activity>
        <activity android:name=".AtSong"></activity>
        <activity android:name=".AtGenre">

        </activity>
        <activity android:name=".AtSubgenre"></activity>
        <activity android:name=".AtLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AtRegister">
        </activity>
        <activity android:name=".AtPlayer">
        </activity>
        <activity android:name=".AtUser"></activity>
    </application>

</manifest>